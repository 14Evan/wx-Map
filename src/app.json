  // Window configuration
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#window
  "window": {
    // 类型：HexColor；默认值：#000000
    // 导航栏背景颜色，如"#000000"
    "navigationBarBackgroundColor": "#0094ff",
    // 类型：String；默认值：white
    // 导航栏标题颜色，仅支持 black/white
    "navigationBarTextStyle": "white",
    // 类型：String；默认值：无
    // 导航栏标题文字内容
    "navigationBarTitleText": "我的小程序",
    // 类型：HexColor；默认值：#ffffff
    // 窗口的背景色
    "backgroundColor": "#0094ff",
    // 类型：String；默认值：dark
    // 下拉背景字体、loading 图的样式，仅支持 dark/light
    "backgroundTextStyle": "light",
//    "navigationStyle": "custom",
    // 类型：Boolean；默认值：false
    // 是否开启下拉刷新，详见页面相关事件处理函数。
    // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/app-service/page.html?t=1476197491005#页面相关事件处理函数
    "enablePullDownRefresh": false
  },
  // Tab bar configuration
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#tabBar
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#0094ff",
    "borderStyle": "black",
    "backgroundColor": "#fff",
    "list": [
      {
        "text": "Weather",
        "pagePath": "pages/index/index",
        "iconPath": "images/index.png",
        "selectedIconPath": "images/index_HL.png"
      },
      {
        "text": "Conversion",
        "pagePath": "pages/money/money",
        "iconPath": "images/money.png",
        "selectedIconPath": "images/money_HL.png"
      },
      {
        "text": "图FUN",
        "pagePath": "pages/photo/joke",
        "iconPath": "images/joke.png",
        "selectedIconPath": "images/joke_hl.png"
      },
      {
        "text": "AboutMe",
        "pagePath": "pages/ddemo/ddemo",
        "iconPath": "images/hi.png",
        "selectedIconPath": "images/hi_HL.png"
      }
    ]
  },
  // Network timeout configuration
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#networkTimeout
  "networkTimeout": {
    "request": 10000,
    "downloadFile": 10000
  },
  // Debug log to console
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#debug
  "debug": true
